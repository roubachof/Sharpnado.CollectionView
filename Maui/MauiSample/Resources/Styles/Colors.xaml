<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <Color x:Key="DarklessSurface">#3D3D3D</Color>

    <Color x:Key="DarkCardBorder">#3A3A3A</Color>

    <Color x:Key="DarkSurface">#383838</Color>

    <Color x:Key="DarkerSurface">#323232</Color>

    <Color x:Key="LightSurface">#FAF8F9</Color>
    <Color x:Key="AcrylicSurface">#E4E4E4</Color>
    <Color x:Key="NeumorphismSurface">#F0F0F3</Color>

    <Color x:Key="Accent">#FF0266</Color>

    <Color x:Key="TransparentAccent">#00121212</Color>

    <Color x:Key="PrimaryColor">Black</Color>
    <Color x:Key="SecondaryColor">#60000000</Color>
    <Color x:Key="TernaryColor">#30000000</Color>

    <Color x:Key="PrimaryDarkColor">White</Color>
